{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "7e655094-9987-4eb1-a526-2fe901a5814c",
        "TimeOut": "900:00:00",
        "ServerRole": "Single",
        "SanitizeTinyMce": true,
        "UseHttps": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Hosting": {
          "Debug": true
        },
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentKey": "9db112c5-c2ea-441d-8bd4-6daf522aa2b6"
          }
        ]
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Tours": {
        "EnableTours": false
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "ExportHtmlSettings": {
    "TargetUrl": "https://huseyinsekmenoglu.net/",
    "OutputFolder": "D:\\Projects",
    "AdditionalUrls": [
      "/sitemap.xml",
      "/thank-you",
      "/404.html",
      "/robots.txt",
      "/humans.txt",
      "/manifest.json"
    ]
  }
}